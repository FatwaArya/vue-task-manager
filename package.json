    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/vue-query": "^5.56.2",
