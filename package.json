    "primeicons": "^7.0.0",
    "primevue": "^4.3.9",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@tailwindcss/postcss": "^4.1.13",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
